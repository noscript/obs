<services>
  <service name="tar_scm">
    <param name="scm">git</param>
    <param name="url">https://github.com/noscript/vim-obs</param>
    <param name="filename">obs</param>
    <param name="versionformat">%h</param>
    <param name="extension">tar_</param>
    <param name="extract">debian.changelog</param>
    <param name="extract">debian.compat</param>
    <param name="extract">debian.control</param>
    <param name="extract">debian.rules</param>
    <param name="extract">vim.dsc</param>
  </service>
  <service name="tar_scm">
    <param name="scm">git</param>
    <param name="url">https://github.com/vim/vim</param>
    <param name="filename">vim</param>
    <param name="versionrewrite-pattern">v(.*)</param>
    <param name="versionformat">@PARENT_TAG@.git@TAG_OFFSET@</param>
    <param name="versionrewrite-replacement">2:\1</param>
  </service>
  <service name="recompress">
    <param name="file">_service:tar_scm:vim-*.tar</param>
    <param name="compression">gz</param>
  </service>
  <service name="set_version"/>
</services>
